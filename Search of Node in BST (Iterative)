
// Code of Node Search in Code Studio



bool searchInBST(BinaryTreeNode<int> *root, int x) {
BinaryTreeNode<int>*temp=root;
while(temp!=NULL){
    if(temp->data==x)
       return true;
    else if(temp->data>x
       return searchInBST(root->left,x)
    else
       return searchInBST(root->right,x);
}
return false;
}

